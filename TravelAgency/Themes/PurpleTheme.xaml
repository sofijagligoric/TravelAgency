<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <Color x:Key="PrimaryColor">#d934ad</Color>
    <Color x:Key="SecondaryColor">#c22f9b</Color>
    <Color x:Key="MouseOverColor">#ab0a80</Color>
    <Color x:Key="ForegroundColor">#fadcf2</Color>


    <SolidColorBrush x:Key="PrimaryBrush" Color="{DynamicResource PrimaryColor}" />
    <SolidColorBrush x:Key="SecondaryBrush" Color="{DynamicResource SecondaryColor}" />
    <SolidColorBrush x:Key="MouseOverBrush" Color="{DynamicResource MouseOverColor}" />
    <SolidColorBrush x:Key="ForegroundBrush" Color="{DynamicResource ForegroundColor}" />

    

</ResourceDictionary>